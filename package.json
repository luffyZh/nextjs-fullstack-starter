{
  "name": "next-fullstack-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"-r esm\" next -- -p 3888",
    "build": "cross-env NODE_OPTIONS=\"-r esm\" next build",
    "analyze": "cross-env-shell NODE_OPTIONS=\"-r esm\" ANALYZE=true next build",
    "prod": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@next/bundle-analyzer": "^9.3.6",
    "antd": "^4.2.0",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.2",
    "esm": "^3.2.25",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.26.5"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/mysql": "^2.15.10",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "typescript": "3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{scss,less,css}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,ts,tsx,js,jsx,md,scss,less,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "ISC"
}
